{
    "extends": "airbnb",
    "globals": {
      "expect": true,
      "beforeAll": true,
      "afterAll": true
    },
    "rules": {
      "react/jsx-filename-extension": [1, {
        "extensions": [".js", ".jsx", ".mjs"]
      }],
      "no-unused-vars": 0
    },
    "env": {
      "es6": true,
      "browser": true,
      "mocha": true
    },
    "parser": "babel-eslint"
}
